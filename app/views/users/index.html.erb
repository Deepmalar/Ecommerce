<div class="container">   
<h1>User List</h1></br>
    
<table border="2">   
  <thead>   
  <tr>   
    <th>Email</th>   
    <th>First_Name</th>   
    <th>Last_name</th>   
    <th>Phone_No.</th>

  </tr>   
  </thead>   
  <tbody>   
  <% @users.each do |user| %>   
      <tr>   
        <td><%= user.email %></td>   
        <td><%= user.first_name %></td> 
        <td><%= user.last_name %></td> 
        <td><%= user.phone_no %></td>  
        <div>   
          <td>   
          <%= link_to 'Show', user_path(user) %>   
          <%= link_to 'Edit', edit_user_path(user) %>   
          <%= link_to 'Delete', user_path(user), method: :delete %>   
        </div>   
        </td>   
      </tr>   
  <% end %>   
  </tbody>   
</table>   
</div>   


