<%= form_with(model: address) do |form| %>
    <div class="field">
     <%= form.label :city %><br>
     <%= form.text_field :city %>
    </div>
   
    <div class="field"> 
     <%= form.label :state %><br>
     <%= form.text_field :state %>
    </div>
      
    <div class="field">   
     <%= form.label :country %><br>
     <%= form.text_field :country %>  
    </div>   
   
    <div class="field">   
     <%= form.label :zip_code %><br>
     <%= form.text_field :zip_code %>   
    </div>  

    <div class="field">   
     <%= form.label :street %><br>
     <%= form.text_field :street %>   
    </div>
     
    <div class="actions">
     <%= form.submit %>
    </div>
   
<% end %>
     